            dataType: 'JSONP',
            success: function (result) {
                var data = JSON.parse(result);
                initGeetest({
                    gt: data.gt,
                    challenge: data.challenge,
                    product: "float",
                    offline: !data.success
                }, handlerPopup);
            }
        })
    }


    seajs.use(['register/stepOne_774e547'], function (register) {
        register.stepOne();//开始注册
